{
  "name": "magacinbe",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "dev-tsnd": "tsnd --respawn src/index.ts",
    "dev": "nodemon --watch src --ext ts,json --exec \"npx ts-node -r tsconfig-paths/register src/index.ts\"",
    "build": "tsc -p tsconfig.prod.json",
    "test": "jest",
    "test:unit": "jest unit",
    "test:int": "jest int",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "sequelize": "sequelize",
    "migrate": " sequelize-cli db:migrate",
    "generate-models": "ts-node src/sequelize-auto-config.ts",
    "run-migrations": "npx sequelize-cli db:migrate",
    "db:migrate": "npx sequelize-cli db:migrate --config sequelize.config.js",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize-cli db:seed:all",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config sequelize.config.js",
    "seed:undo": "ts-node ./node_modules/.bin/sequelize db:seed:undo:all",
    "db:seed:all": "npx sequelize-cli db:seed:all --config sequelize.config.js",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --config sequelize.config.js"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.5",
    "@types/pg": "^8.11.11",
    "@types/sequelize": "^4.28.20",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.0",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.2",
    "sequelize-cli-ts-js": "^1.1.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
